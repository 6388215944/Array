import java.util.*;
/**
 * First
 */
public class First {

    public static void main(String[] args) {
       int arr[]={2 , 4 , 7 , 6 , 4 ,10};
       Scanner sc = new Scanner(System.in);
       int x = sc.nextInt();

       for( int i=0; i<arr.length; i++){
        if(arr[i]==x){
            System.out.println("x found at index: " + i);
        }
       }
       

        }

      
        }
        
  

     


